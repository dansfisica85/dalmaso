{
  "manifest_version": 3,
  "name": "Extrator SED - Dados de Alunos",
  "description": "Extrai dados pessoais e telefones dos alunos do sistema SED",
  "version": "1.0",
  "permissions": ["activeTab", "storage", "downloads"],
  "content_scripts": [
    {
      "matches": ["*://sed.educacao.sp.gov.br/*", "*://*.educacao.sp.gov.br/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_idle",
      "world": "MAIN"
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
